# include <stdio.h>
int main()
{
   int x,y;
   printf ("enter the first num");
   scanf("%s",&x);
   printf("enter the second number");
   scanf("%d",&y);
   if(x>y)
{
    printf("%d is the largest number.\n",x);
} 
else if (x>y)
{
    printf("%d is the largest num.\n",y);
}
 else{
     printf("both numbers are equal.\n");
 }
 return 0;
}
 
   
    




    

